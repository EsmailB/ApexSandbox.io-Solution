public boolean isAnagram(String s1, String s2) {
    //Code here
    if (s1.length() != s2.length() || (s1 == null || s2 == null))
        return false;
    
    Set<String> set_s1 = new Set<String>(s1.split(''));
    List<String> list_s2 = s2.split('');
    
    if (set_s1.containsAll(list_s2))
        return true;
    else 
        return false;
    
}