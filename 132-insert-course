public Id insertCourse(String name, String details, Integer credits) {
    // code here

    apxio__Course__c course = new apxio__Course__c();
    course.name = name;
    course.apxio__Course_Details__c = details;
    
    if (course.apxio__Credits__c >=1 && course.apxio__Credits__c <=4)
    {
        course.apxio__Credits__c = credits;
    
        insert course;
        return course.id;
    }

    return null;
}