public Integer nextPrime(Integer num) {
    //code here

    integer i = num;
    while(!isPrime(i))
        i++;

    return i;
 
}

public Boolean isPrime(Integer num) {
    //code here

    for(Integer i = 2; i < num; i++)
        if (Math.mod(num, i) == 0)
            return false;

    return true;
}