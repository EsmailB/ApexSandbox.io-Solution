public Map<String, List<Case>> casesByType(List<Case> cases) {
       //code here
        Map<String, List<Case>> result = new Map<String, List<Case>>();

        for(Case c: casesByType)
        {
            if (c.Type != 'null')
            {
                if (result.containsKey(c.Type)) // add to list
                    {
                        List<String> tmp = result.get(c.Type);
                        tmp.add(c.Subject);
                        result.put(c.Type, tmp);
                    }
                else // create entry and add to list
                    result.put(c.Type, new List<String>{c.Subject})  ;
            }
        }

        return result;
    }
