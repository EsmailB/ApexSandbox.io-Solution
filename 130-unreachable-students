public List<apxio__Student__c> selectUnreachableStudents() {
    // code here

        List<apxio__Student__c> result = new List<apxio__Student__c>();
        
        for(apxio__Student__c s : [SELECT Id, Name, apxio__Phone__c, apxio__Email__c FROM apxio__Student__c WHERE apxio__Active__c = TRUE])
            if (s.apxio__Phone__c == null  && s.apxio__Email__c == null)
                result.add(s);        

        result.sort();
        return result;
    }